{
  "name": "stakhanovist/queue",
  "description": "Provides a factory function to create specific queue client objects",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Leonardo Grasso",
      "email": "me@leonardograsso.com",
      "homepage": "http://github.com/leogr"
    },
    {
      "name": "Leonardo Di Donato",
      "email": "leodidonato@gmail.com",
      "homepage": "http://github.com/leodido"
    }
  ],
  "keywords": [
    "zf2",
    "queue"
  ],
  "homepage": "https://github.com/stakhanovist/queue",
  "autoload": {
    "psr-4": {
      "Stakhanovist\\Queue\\": "library"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StakhanovistQueueTest\\": "tests"
    }
  },
  "require": {
    "php": ">=5.5",
    "zendframework/zend-stdlib": "~2.1",
    "zendframework/zend-servicemanager": "~2.1",
    "zendframework/zend-math": "~2.1",
    "zendframework/zend-eventmanager": "~2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7",
    "satooshi/php-coveralls": "dev-master",
    "zendframework/zend-db": "~2.1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev",
      "dev-develop": "0.2.x-dev"
    }
  },
  "suggest": {
    "zendframework/zend-db": "Zend\\Db required if you are using the DB queue adapter",
    "ext-mongo": "Mongo required if you are using the Mondo queue adapter"
  }
}
