{
    "name": "stakhanovist/queue",
    "description": "Provides a factory function to create specific queue client objects",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "queue"
    ],
    "homepage": "https://github.com/stakhanovist/queue",
    "autoload": {
        "psr-4": {
            "Stakhanovist\\Queue\\": "library",
            "StakhanovistQueueTest\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "dev-master",
        "phpmd/phpmd": "1.4.*",
        "pdepend/pdepend": "1.1.*",
        "fabpot/php-cs-fixer": "*@dev",
    	"zendframework/zendframework" : "~2"
    },
    "require": {
        "php": ">=5.3.23",
        "zendframework/zend-stdlib": "~2",
        "zendframework/zend-servicemanager": "~2",
        "zendframework/zend-math": "~2",
        "zendframework/zend-eventmanager": "~2"
    },
    "suggest": {
        "zendframework/zend-db": "Zend\\Db required if you are using the DB queue adapter",
        "ext-mongo": "Mongo required if you are using the Mondo queue adapter"
    }
}
